<% SocialStream.extended_search_models.each do |model_sym| %>
<div class="subject_search_results block  <%= cycle "left", "right" %>">
  <div class="title">
    <%= model_sym.to_s.capitalize.pluralize %>
  </div>
  <% if @search_result[model_sym].empty? %>
  <div class="model_with_details">
    <%= I18n.t('search.no_subject_found', :subject => model_sym.to_s) %>
  </div>
  <% else %>
  <% @search_result[model_sym].each do |subject|%>
  <%= model_with_details subject %>
  <% end %>
  <div class="model_with_details">
    <%= focus_search_link(I18n.t('search.all_results', :subject => model_sym.to_s.pluralize, :count => @search_result[(model_sym.to_s+"_total").to_sym].to_s),model_sym.to_s,params[:search_query]) %>
  </div>
  <% end %>
</div>
<% end %> 