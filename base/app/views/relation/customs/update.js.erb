<% if params[:section] == 'permissions' %>
  <%= render :partial => 'update_permissions' %>
<% else %>
  <% if @custom.valid? %>
    SocialStream.RelationCustom.update({
      relation: {
        id: <%= @custom.id %>,
        name: "<%= @custom.name %>"
      },
      section: 'edit_name'
    });
  <% else %>
    
  <% end %>
<% end %>


